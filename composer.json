{
  "name": "highstan/highstan",
  "description": "Higher kinded types for PHPStan",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Klimenko Andrew",
      "email": "klimichkartorgnusov@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.85",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-strict-rules": "^2.0",
    "phpyh/coding-standard": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Highstan\\": "src/"
    }
  },
  "config": {
    "bump-after-update": "dev",
    "sort-packages": true
  }
}
